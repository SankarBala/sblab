  margin-right: 0;
}
.header__nav nav ul li a {
  display: block;
  font-size: 16px;
  color: #9a9fad;
  font-family: "Poppins", sans-serif;
  padding: 10px 5px;
  -webkit-transition: all, 0.3s;
  -moz-transition: all, 0.3s;
  -ms-transition: all, 0.3s;
